naming Abrevation used in project:

    gcv_modelname   : gcv_lr , gcv_dct
    gcv_modelname_d : gcv_lr_d, 
    kfold
    
    modal name : 
        LogisticRegression        : lr
        KNNeighbourClassification : knn
        
    
    Scalar :
        StandardScalar            : std_scalar
        MinMaxScalar              : mm_scalar
    
    
    best_score :
        default score of lr model     : d_score_lr
        best score of tunned lr model : best_score_lr
        default param of lr model     : d_param_lr
        best param of tunned lr model : best_param_lr
    
    